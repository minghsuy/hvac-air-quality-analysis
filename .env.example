# Airthings API Credentials
# Get these from https://accounts.airthings.com/clients
AIRTHINGS_CLIENT_ID=your_client_id_here
AIRTHINGS_CLIENT_SECRET=your_client_secret_here

# Device Serial Numbers
# Find in Airthings app or dashboard
AIRTHINGS_DEVICE_SERIAL=your_airthings_serial_here

# Outdoor AirGradient sensor
# Find via: avahi-browse -t _http._tcp or check DHCP leases
AIRGRADIENT_SERIAL=your_airgradient_serial_here

# Indoor AirGradient sensor (for second bedroom)
AIRGRADIENT_INDOOR_SERIAL=your_airgradient_indoor_serial_here

# AirGradient sensor IPs (run scripts/update_airgradient_ips.py to discover)
AIRGRADIENT_OUTDOOR_IP=192.168.X.XX
AIRGRADIENT_INDOOR_IP=192.168.X.XX

# Google Sheets API
# See wiki Software-Setup for full setup instructions
GOOGLE_SPREADSHEET_ID=your_spreadsheet_id_here
GOOGLE_SHEET_TAB=Cleaned_Data_20250831

# Temp Stick WiFi sensor (attic temperature/humidity)
# Get API key from https://tempstickapi.com
TEMP_STICK_API_KEY=your_api_key_here
TEMP_STICK_SENSOR_ID=your_sensor_id_here
